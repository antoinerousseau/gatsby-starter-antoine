  "license": "MIT",
  "scripts": {
    "develop": "rm -rf .cache && rm -rf public/ && gatsby develop",
    "lambda": "NODE_ENV=development netlify-lambda serve src/functions --config webpack.functions.js --timeout 30",
    "lint": "tsc --skipLibCheck ; eslint --fix \"**/*.{ts,tsx,js,jsx,json}\"",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "build": "rm -rf .cache && rm -rf public/ && gatsby build && netlify-lambda build src/functions --config webpack.functions.js",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/antoinerousseau/gatsby-starter-antoine"
